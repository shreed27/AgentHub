# Trading Orchestrator Production Environment Variables
# Copy to Railway Dashboard > Variables

# =============================================================================
# SERVER CONFIGURATION
# =============================================================================
PORT=4001
NODE_ENV=production

# =============================================================================
# INTERNAL SERVICE URLs (Railway Private Networking)
# =============================================================================
AGENT_DEX_URL=http://agent-dex.railway.internal:3001
CLODDSBOT_URL=http://cloddsbot.railway.internal:18789
OPENCLAW_URL=http://openclaw.railway.internal:3003
OPUS_X_URL=https://superrouter.fun
CLAWDNET_URL=https://clawdnet.xyz
OSINT_MARKET_URL=

# =============================================================================
# BLOCKCHAIN CONFIGURATION
# =============================================================================
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_NETWORK=mainnet-beta

# =============================================================================
# SECURITY (Required - generate with: openssl rand -hex 32)
# =============================================================================
JWT_SECRET=
ENCRYPTION_KEY=

# =============================================================================
# FEATURE FLAGS
# =============================================================================
ENABLE_CLAWDNET=true
ENABLE_OSINT=true
ENABLE_WHALE_TRACKING=true
ENABLE_AI_ANALYSIS=true

# =============================================================================
# RISK LIMITS (Global Defaults)
# =============================================================================
DEFAULT_MAX_POSITION_SIZE=1000
DEFAULT_MAX_DAILY_LOSS=500
DEFAULT_MAX_OPEN_POSITIONS=5
